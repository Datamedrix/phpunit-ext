{
    "name": "dmx/phpunit-ext",
    "description": "A collection of constraints, asserts and test-cases for the PHPUnit framework.",
    "keywords": ["phpunit", "enhancements", "extension", "constraints"],
    "license": "MIT",
    "type": "library",
    "version": "1.2.0",
    "require": {
        "php": ">=7.4.1",
        "phpunit/phpunit": "^7.0|^8.0",
        "nesbot/carbon": "^1.25.0|^2.10"
    },
    "require-dev": {
        "ext-mbstring": "*",
        "friendsofphp/php-cs-fixer": "^2.14"
    },
    "autoload": {
        "psr-4": {
            "DMX\\PHPUnit\\Framework\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DMX\\PHPUnit\\Framework\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "style-fix": "vendor/bin/php-cs-fixer fix",
        "style-check": "vendor/bin/php-cs-fixer fix --dry-run --using-cache=no"
    },
    "config": {
        "preferred-install": "dist"
    }
}
